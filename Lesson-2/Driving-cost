import java.util.Scanner;

public class LabProgram {
   public static double drivingCost(double milesPerGallon,
   double dollarsPerGallon, double milesDriven){
      return ((milesDriven/milesPerGallon)*dollarsPerGallon);
      }
   
   
   public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      double milesGallon = scan.nextDouble();
       double DollarsGallon = scan.nextDouble();
double result1 = drivingCost(milesGallon,DollarsGallon,10);
 double result2=drivingCost( milesGallon,DollarsGallon,50);
       double result3 = drivingCost(milesGallon,DollarsGallon,400);
      System.out.printf("%.2f ",result1);
      System.out.printf("%.2f ", result2);
      System.out.printf("%.2f", result3);
      System.out.println();
   }
}
